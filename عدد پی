#include <stdio.h>

int main() {
  // تعریف یک متغیر double برای ذخیره عدد پی
  double pi = 3.14159265358979323846;

  // گرد کردن عدد پی به چهار رقم اعشار
  double rounded_pi = round(pi * 10000) / 10000;

  // چاپ عدد پی گرد شده
  printf("عدد پی با چهار رقم اعشار برابر با %.4lf است.\n", rounded_pi);

  return 0;
}
